{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "baseUrl": ".",
    "paths": {
      "@/db/*": ["./src/db/*"],
      "@/config/*": ["./src/config/*"],
      "@/entities/*": ["./src/entities/*"],
      "@/entities": ["./src/entities/index.ts"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
